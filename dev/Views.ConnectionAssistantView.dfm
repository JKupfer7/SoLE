        end
        object Label7: TLabel
          Left = 67
          Top = 175
          Width = 440
          Height = 68
          Anchors = [akLeft, akTop, akRight]
          AutoSize = False
          Caption = 
            'MariaDB was created as a fork of MySQL. It is almost fully compa' +
            'tible with MySQL, offers the same functionality and has the same' +
            ' deployment requirements. MariaDB is used in many Linux distribu' +
            'tions. If you know that MariaDB is used in your institution, you' +
            ' should select this option.'
          WordWrap = True
        end
        object Label8: TLabel
          Left = 67
          Top = 279
          Width = 440
          Height = 68
          Anchors = [akLeft, akTop, akRight]
          AutoSize = False
          Caption = 
            'SQLite does not require installation of server software or a use' +
            'r account. However, this option does not allow shared work on th' +
            'e local network. SQLite should be used only for testing purposes' +
            ' or when the other two options are not applicable.'
          WordWrap = True
        end
